{
  "name": "@pageobject/examples",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "report": "allure generate allure-results --clean -o allure-report",
    "postreport": "s3-website deploy --lock-config",
    "pretest": "rm -rf allure-report && rm -rf allure-results",
    "test": "multi='progress=- mocha-allure-reporter=-' mocha --async-only --timeout 15000 --ui tdd --reporter mocha-multi './lib/**/*.test.js'",
    "posttest": "node src/quick-start/test.js"
  },
  "dependencies": {
    "@pageobject/class": "^0.2.0",
    "@pageobject/selenium-adapter": "^0.2.0",
    "@types/selenium-webdriver": "^3.0.8",
    "selenium-webdriver": "^3.6.0"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.44",
    "allure-commandline": "^2.3.5",
    "mocha": "^4.0.1",
    "mocha-allure-reporter": "^1.3.2",
    "mocha-multi": "^0.11.1",
    "s3-website": "^3.2.2"
  }
}
